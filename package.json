{
  "name": "telegram-gemini-bot",
  "version": "1.0.1",
  "description": "A Telegram bot for images",
  "main": "bot.mjs",
  "scripts": {
    "start": "node bot.mjs",
    "dev": "nodemon bot.mjs",
    "webhook:set": "node set-webhook.js set",
    "webhook:delete": "node set-webhook.js delete",
    "webhook:info": "node set-webhook.js info",
    "webhook:local": "node set-webhook.js set http://localhost:3000/webhook"
  },
  "dependencies": {
    "@google/generative-ai": "^0.16.0",
    "axios": "^1.7.3",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "node-telegram-bot-api": "^0.66.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
